<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>session_admin</servlet-name>
        <servlet-class>Servlet.session_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registro_admin</servlet-name>
        <servlet-class>Servlet.registro_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>docentes_admin</servlet-name>
        <servlet-class>Servlet.docentes_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cursos_admin</servlet-name>
        <servlet-class>Servlet.cursos_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>relacion_admin</servlet-name>
        <servlet-class>Servlet.relacion_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>clases_admin</servlet-name>
        <servlet-class>Servlet.clases_admin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>session_admin</servlet-name>
        <url-pattern>/session_admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registro_admin</servlet-name>
        <url-pattern>/registro_admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>docentes_admin</servlet-name>
        <url-pattern>/docentes_admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cursos_admin</servlet-name>
        <url-pattern>/cursos_admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>relacion_admin</servlet-name>
        <url-pattern>/relacion_admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>clases_admin</servlet-name>
        <url-pattern>/clases_admin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
